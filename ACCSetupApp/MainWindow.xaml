<Window x:Class="ACCSetupApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ACCSetupApp"
        xmlns:controls="clr-namespace:ACCSetupApp.Controls"
        mc:Ignorable="d"
        Title="MainWindow" Width="1000" MinWidth="1025" MinHeight="500" Background="Black">
    <Grid MaxWidth="1050" HorizontalAlignment="Left" Background="Black">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <controls:SetupRenderer Grid.Column="0"></controls:SetupRenderer>
        <controls:SetupRenderer Grid.Column="1"></controls:SetupRenderer>
    </Grid>
</Window>
